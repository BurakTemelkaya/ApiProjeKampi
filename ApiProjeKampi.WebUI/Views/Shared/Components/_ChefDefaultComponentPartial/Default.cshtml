@model List<ResultChefDto>

<section id="chefs" class="chefs section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Şefler</h2>
        <p><span>Mutfaktaki</span> <span class="description-title">Profesyonel Şeflerimiz<br></span></p>
    </div><!-- End Section Title -->

    <div class="container">

        <div class="row gy-4">

            @foreach (ResultChefDto chef in Model)
            {
                <div class="col-lg-4 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="100">
                    <div class="team-member">
                        <div class="member-img">
                            <img src="@chef.ImageUrl" class="img-fluid" alt="">
                            <div class="social">
                                <a href=""><i class="bi bi-twitter-x"></i></a>
                                <a href=""><i class="bi bi-facebook"></i></a>
                                <a href=""><i class="bi bi-instagram"></i></a>
                                <a href=""><i class="bi bi-linkedin"></i></a>
                            </div>
                        </div>
                        <div class="member-info">
                            <h4>@chef.NameSurname</h4>
                            <span>@chef.Title</span>
                            <p>@chef.Description</p>
                        </div>
                    </div>
                </div>
            }

        </div>

    </div>

</section>